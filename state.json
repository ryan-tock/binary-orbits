      {
        "type": "expression",
        "id": "23",
        "folderId": "51",
        "color": "#388c46",
        "latex": "M\\left(t\\right)=M_{0}+\\frac{2\\pi}{p}\\left(t-2000\\right)"
      },
      {
        "type": "text",
        "id": "119",
        "folderId": "51",
        "text": "Recursive Newton-Raphson iterator for eccentric anomaly"
      },
      {
        "type": "expression",
        "id": "10",
        "folderId": "51",
        "color": "#aaaaaa",
        "latex": "E_{guess}\\left(t,n\\right)=\\left\\{n=0:M\\left(t\\right),E_{guess}\\left(t,n-1\\right)+\\frac{\\left(M\\left(t\\right)-E_{guess}\\left(t,n-1\\right)+e_{0}\\sin\\left(E_{guess}\\left(t,n-1\\right)\\right)\\right)}{1-e_{0}\\cos\\left(E_{guess}\\left(t,n-1\\right)\\right)}\\right\\}"
      },
      {
        "type": "expression",
        "id": "11",
        "folderId": "51",
        "color": "#c74440",
        "latex": "M_{guess}\\left(t,n\\right)=E_{guess}\\left(t,n\\right)-e_{0}\\sin\\left(E_{guess}\\left(t,n\\right)\\right)"
      },
      {
        "type": "text",
        "id": "117",
        "folderId": "51",
        "text": "Eccentric Anomaly"
      },
      {
        "type": "expression",
        "id": "14",
        "folderId": "51",
        "color": "#6042a6",
        "latex": "E\\left(t\\right)=E_{guess}\\left(t,i_{terations}\\right)"
      },
      {
        "type": "expression",
        "id": "15",
        "folderId": "51",
        "color": "#aaaaaa",
        "latex": "e_{rror}\\left(t\\right)=\\left|M\\left(t\\right)-M_{guess}\\left(t,i_{terations}\\right)\\right|"
      },
      {
        "type": "expression",
        "id": "17",
        "folderId": "51",
        "color": "#2d70b3",
        "latex": "\\beta=\\frac{e_{0}}{1+\\sqrt{1-e_{0}^{2}}}"
      },
      {
        "type": "text",
        "id": "121",
        "folderId": "51",
        "text": "True Anomaly"
      },
      {
        "type": "expression",
        "id": "18",
        "folderId": "51",
        "color": "#388c46",
        "latex": "T\\left(t\\right)=E\\left(t\\right)+2\\arctan\\left(\\frac{\\beta\\sin\\left(E\\left(t\\right)\\right)}{1-\\beta\\cos\\left(E\\left(t\\right)\\right)}\\right)"
      },
      {
        "type": "expression",
        "id": "16",
        "folderId": "51",
        "color": "#c74440",
        "latex": "r_{0}\\left(E\\right)=a\\left(1-e_{0}\\cos\\left(E\\right)\\right)"
      },
      {
        "type": "expression",
        "id": "8",
        "folderId": "51",
        "color": "#388c46",
        "latex": "x_{1}\\left(r,f\\right)=r\\left(\\cos\\left(f+\\omega\\right)\\cos\\left(\\Omega-\\frac{3\\pi}{2}\\right)-\\cos\\left(i\\right)\\sin\\left(f+\\omega\\right)\\sin\\left(\\Omega-\\frac{3\\pi}{2}\\right)\\right)"
      },
      {
        "type": "expression",
        "id": "9",
        "folderId": "51",
        "color": "#6042a6",
        "latex": "y_{1}\\left(r,f\\right)=r\\left(\\cos\\left(i\\right)\\sin\\left(f+\\omega\\right)\\cos\\left(\\Omega-\\frac{3\\pi}{2}\\right)+\\cos\\left(f+\\omega\\right)\\sin\\left(\\Omega-\\frac{3\\pi}{2}\\right)\\right)"
      },
      {
        "type": "expression",
        "id": "20",
        "folderId": "51",
        "color": "#aaaaaa",
        "latex": "z_{1}\\left(r,f\\right)=r\\sin\\left(f+\\omega\\right)\\sin\\left(i\\right)"
      },
      {
        "type": "expression",
        "id": "71",
        "folderId": "51",
        "color": "#388c46",
        "latex": "p_{os}\\left(t\\right)=\\left(\\left(x_{1}\\left(r_{0}\\left(E\\left(t\\right)\\right),T\\left(t\\right)\\right),y_{1}\\left(r_{0}\\left(E\\left(t\\right)\\right),T\\left(t\\right)\\right),z_{1}\\left(r_{0}\\left(E\\left(t\\right)\\right),T\\left(t\\right)\\right)\\right)\\right)"
      },
